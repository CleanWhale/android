<?xml version="1.0" encoding="UTF-8"?>
<manifest>

 <remote  name="github"
          fetch="https://github.com/" />

 <remote  name="aosp"
          fetch="https://android.googlesource.com"
          review="android-review.googlesource.com" />

 <remote  name="cleanwhale"
          fetch="https://github.com/CleanWhale/"
          revision="master" />
           
 <default revision="refs/tags/android-6.0.1_r46"
          remote="aosp"
          sync-c="true"
          sync-f="true"
          sync-j="8" />

<project path=" build" name="android_build" remote="cleanwhale" />
<project path="manifest" name="android" remote="cleanwhale" />
<project path="device/moto/shamu" name="android_device_moto_shamu" remote="cleanwhale" />
<project path="kernel/moto/shamu" name="android_kernel_moto_shamu" remote="cleanwhale" />
<project path="vendor/moto" name="android_vendor_moto" remote="cleanwhale" />
</manifest>
